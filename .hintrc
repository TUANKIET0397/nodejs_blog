{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "disown-opener": "off",
    "button-type": "off",
    "axe/forms": "off",
    "axe/text-alternatives": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off"
  }
}